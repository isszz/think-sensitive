{
    "name": "isszz/think-sensitive",
    "description": "thinkphp6+ 敏感词检测，过滤",
    "license": "MIT",
    "authors": [
        {
            "name": "isszz",
            "email": "isszz@qq.com"
        }
    ],
    "require": {
        "php": ">=8.0.0",
        "topthink/framework": "8.*|6.0.*"
    },
    "autoload": {
        "files": [
            "src/helpers.php"
        ],
        "psr-4": {
            "isszz\\sensitive\\": "src"
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "extra": {
        "branch-alias": {
            "dev-master": "dev-dev"
        },
        "think": {
            "services":[
                "isszz\\sensitive\\Service"
            ],
            "config": {
                "sensitive": "src/config/config.php"
            }
        }
    }
}
